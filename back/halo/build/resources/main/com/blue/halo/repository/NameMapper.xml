<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.blue.halo.repository.NameMapper">

    <select id="getAnswer" resultType="com.blue.halo.Dto.IdNameDto">
        select * from namelist where id=#{idNumber}
    </select>

    <select id="getWord" resultType="com.blue.halo.Dto.IdWordDto">
        select * from nameword where id=#{idNumber}
    </select>

</mapper>